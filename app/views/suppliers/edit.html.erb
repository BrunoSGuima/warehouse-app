<h2> Editar Fornecedores </h2>

<%= form_with(model: @supplier) do |f| %>
    <div>
      <%= f.label :corporate_name %>
      <%= f.text_field :corporate_name %>
		</div> <br>	

		<div>
			<%= f.label :brand_name %>
      <%= f.text_field :brand_name %>
		</div> <br>
		
		<div>
			<%= f.label :full_address %>
      <%= f.text_field :full_address %>
		</div> <br>

		<div>	
			<%= f.label :registration_number %>
      <%= f.text_field :registration_number %>
		</div> <br>

		<div>	
			<%= f.label :city %>
      <%= f.text_field :city %>
		</div>	<br>

		<div>
			<%= f.label :state %>
      <%= f.text_field :state %>
		</div>	<br>

		<div>
			<%= f.label :email %>
      <%= f.text_field :email %>
		</div>	<br>

		<div>
			<%= f.submit 'Enviar' %>
		</div>

 <% end %>